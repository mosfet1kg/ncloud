{
  "name": "ncloud",
  "version": "0.1.7",
  "description": "The NAVER Cloud Platform(https://www.ncloud.com/) Library for Node.js",
  "engineStrict": true,
  "engines": {
    "node": ">= 6.9.0"
  },
  "main": "./lib/index.js",
  "devDependencies": {
    "@types/jest": "~19.2.3",
    "@types/node": "~6.0.73",
    "jest": "~20.0.0",
    "rimraf": "~2.6.1",
    "tslib": "^1.7.1",
    "tslint": "~4.5.1",
    "tslint-config-airbnb": "^5.1.2",
    "tslint-microsoft-contrib": "~4.0.1",
    "typescript": "^2.5.3"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp lib",
    "clean:lib": "rimraf lib",
    "prebuild": "npm run clean:lib",
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -w -p tsconfig.json",
    "lint": "tslint -t stylish '{src,__tests__}/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "deploy": "npm publish"
  },
  "author": "Gyubeom Choi <mosfet1kg@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mosfet1kg/ncloud.git"
  },
  "keywords": [
    "ncloud",
    "ncp",
    "NAVER Cloud Platform"
  ],
  "dependencies": {
    "axios": "^0.16.2",
    "crypto-js": "^3.1.9-1",
    "ip": "^1.1.5",
    "validator": "^7.0.0"
  }
}
